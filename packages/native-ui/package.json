{
  "name": "@utilitywarehouse/native-ui",
  "version": "0.12.1",
  "description": "Utility Warehouse React Native UI library",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "module": "build/index",
  "sideEffects": false,
  "scripts": {
    "clean": "rm -rf node_modules && rm -rf build",
    "generateColours": "node ./scripts/generateColours.js",
    "lint:fix": "TIMING=1 eslint --fix --max-warnings 0 --ext ts,tsx src",
    "lint": "TIMING=1 eslint --max-warnings 0 --ext ts,tsx src",
    "build": "tsc && tsc --p tsconfig.lab.json",
    "watch": "tsc --watch",
    "watch:lab": "tsc --p tsconfig.lab.json  --watch",
    "figma:create": "figma connect create",
    "figma:publish": "figma connect publish",
    "test": "echo \"Error: no test specified\" && exit 1",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "set NODE_ENV=production & storybook build",
    "storybook:generate": "sb-rn-get-stories --config-path .ondevice"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@expo/html-elements": "latest",
    "@gluestack-ui/accordion": "1.0.7",
    "@gluestack-ui/alert": "0.1.15",
    "@gluestack-ui/button": "1.0.7",
    "@gluestack-ui/checkbox": "0.1.32",
    "@gluestack-ui/icon": "0.1.22",
    "@gluestack-ui/input": "0.1.31",
    "@gluestack-ui/pressable": "0.1.16",
    "@gluestack-ui/radio": "0.1.33",
    "@gluestack-ui/spinner": "0.1.14",
    "@gluestack-ui/switch": "0.1.22",
    "@utilitywarehouse/colour-system": "workspace:^",
    "@utilitywarehouse/design-tokens": "workspace:^",
    "@utilitywarehouse/react-native-icons": "1.12.0",
    "nanoid": "^5.0.8"
  },
  "devDependencies": {
    "@a110/storybook-expand-all": "^1.1.1",
    "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.26.3",
    "@babel/preset-typescript": "^7.26.0",
    "@chromatic-com/storybook": "^3.2.4",
    "@figma/code-connect": "^1.2.4",
    "@storybook/addon-a11y": "^7.6.20",
    "@storybook/addon-docs": "^8.5.3",
    "@storybook/addon-essentials": "^8.5.3",
    "@storybook/addon-interactions": "^8.5.3",
    "@storybook/addon-onboarding": "^8.5.3",
    "@storybook/blocks": "^8.5.3",
    "@storybook/icons": "^1.3.2",
    "@storybook/native-components": "^3.1.2",
    "@storybook/react": "^8.5.3",
    "@storybook/react-vite": "^8.5.3",
    "@storybook/test": "^8.5.3",
    "@types/react": "^18.2.75",
    "@utilitywarehouse/fontsource": "^0.1.0",
    "@vitejs/plugin-react": "^4.3.4",
    "react": "^18.2.0",
    "react-native": "^0.77.0",
    "react-native-edge-to-edge": "^1.1.3",
    "react-native-gesture-handler": "^2.20.2",
    "react-native-nitro-modules": "^0.24.1",
    "react-native-reanimated": "~3.16.5",
    "react-native-unistyles": "3.0.0-beta.8",
    "react-native-web": "^0.19.13",
    "remark-gfm": "^4.0.0",
    "storybook": "^8.5.3",
    "storybook-dark-mode": "^3.0.3",
    "typescript": "^5.5.3",
    "vite-plugin-react-native-web": "^2.1.1"
  },
  "peerDependencies": {
    "react": ">=16",
    "react-dom": ">=16",
    "react-native": ">=0.72",
    "react-native-gesture-handler": "^2.20.0",
    "react-native-reanimated": "3.x",
    "react-native-svg": ">=13.4.0",
    "react-native-unistyles": ">=>3.0.0",
    "react-native-web": ">=0.19"
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release ${version}",
      "tagName": "v${version}"
    },
    "npm": {
      "publish": true
    },
    "github": {
      "release": true
    }
  }
}
