{
  "name": "@utilitywarehouse/figma-variables-plugin",
  "version": "0.0.0",
  "description": "UW Figma variables plugin",
  "license": "ISC",
  "scripts": {
    "build": "webpack --mode=production",
    "build:watch": "webpack --mode=development --watch",
    "clean": "rm -rf node_modules && rm -rf dist",
    "lint:fix": "TIMING=1 eslint --fix --max-warnings 0 --ext ts,tsx src",
    "lint": "TIMING=1 eslint --max-warnings 0 --ext ts,tsx src",
    "postinstall": "node ./postinstall.js",
    "prettier:format": "prettier --write '**/*.{js,jsx,ts,tsx,css,json}' "
  },
  "dependencies": {
    "@mui/material": "5.14.15",
    "@utilitywarehouse/css-reset": "workspace:^",
    "@utilitywarehouse/web-ui": "workspace:^",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@figma/eslint-plugin-figma-plugins": "0.15.0",
    "@figma/plugin-typings": "^1.100.2",
    "@types/react": "^19.1.0",
    "@types/react-dom": "^19.1.0",
    "css-loader": "^6.7.1",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "react-dev-utils": "^12.0.1",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.5.1",
    "typescript": "^4.7.4",
    "url-loader": "^4.1.1",
    "webpack": "^5.95.0",
    "webpack-cli": "^4.10.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,css,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "plugin:@figma/figma-plugins/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "project": "./tsconfig.json"
    },
    "root": true,
    "rules": {
      "@typescript-eslint/no-unused-vars": [
        "error",
        {
          "argsIgnorePattern": "^_",
          "varsIgnorePattern": "^_",
          "caughtErrorsIgnorePattern": "^_"
        }
      ]
    }
  }
}
